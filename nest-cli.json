      }
    },
    "monday": {
      "type": "library",
      "root": "libs/monday",
      "entryFile": "index",
      "sourceRoot": "libs/monday",
      "compilerOptions": {
        "tsConfigPath": "libs/monday/tsconfig.lib.json"
      }
    },
    "nim": {
      "type": "library",
      "root": "libs/llm",
      "entryFile": "index",
      "sourceRoot": "libs/llm",
