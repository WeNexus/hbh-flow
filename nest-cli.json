{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": false,
    "tsConfigPath": "apps/api/tsconfig.app.json"
  },
  "monorepo": true,
  "root": "apps/api",
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.app.json"
      }
    },
    "worker": {
      "type": "application",
      "root": "apps/worker",
      "entryFile": "main",
      "sourceRoot": "apps/worker",
      "compilerOptions": {
        "tsConfigPath": "apps/worker/tsconfig.app.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "workflow": {
      "type": "library",
      "root": "libs/workflow",
      "entryFile": "index",
      "sourceRoot": "libs/workflow",
      "compilerOptions": {
        "tsConfigPath": "libs/workflow/tsconfig.lib.json"
      }
    },
    "auth": {
      "type": "library",
      "root": "libs/auth",
      "entryFile": "index",
      "sourceRoot": "libs/auth",
      "compilerOptions": {
        "tsConfigPath": "libs/auth/tsconfig.lib.json"
      }
    },
    "hub": {
      "type": "library",
      "root": "libs/hub",
      "entryFile": "index",
      "sourceRoot": "libs/hub",
      "compilerOptions": {
        "tsConfigPath": "libs/hub/tsconfig.lib.json"
      }
    },
    "zoho": {
      "type": "library",
      "root": "libs/zoho",
      "entryFile": "index",
      "sourceRoot": "libs/zoho",
      "compilerOptions": {
        "tsConfigPath": "libs/zoho/tsconfig.lib.json"
      }
    },
    "fujimausa": {
      "type": "library",
      "root": "libs/fujimausa",
      "entryFile": "index",
      "sourceRoot": "libs/fujimausa",
      "compilerOptions": {
        "tsConfigPath": "libs/fujimausa/tsconfig.lib.json"
      }
    },
    "woocommerce": {
